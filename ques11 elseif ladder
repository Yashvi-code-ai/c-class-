#include<stdio.h>
int main(){
int marks;
 printf("Enter your marks (0 - 100): ");
 scanf("%d",&marks);

if (marks >=90 && marks <=100) {
printf(" grade is A\n "); }
 else if ( marks >=75 && marks <=89) {
     printf(" grade is B\n "); }
 else if ( marks >=50 && marks <=74) {
     printf(" grade is C\n"); }
else if ( marks>=0 && marks <50){
     printf(" grade is F\n"); }

return 0;
}


